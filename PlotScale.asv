function [Xlimit, Ylimit] = PlotScale(imu_data,time,j)
%PlotScale scales the IMU data on both the x and y axis

if j <= 3
Ylimit = [max(max(imu_data(:,1:3))), min(min(imu_data(:,1:3)))];
else if j <= 6
Ylimit = [max(max(imu_data(:,4:6))), min(min(imu_data(:,4:6)))];



Xlimit = [time(1,length(time)-30*10) time(1,length(time)-30*2)];
end
